<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="260dp"
    android:layout_height="match_parent"
    android:background="#463e3f"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:gravity="center_vertical" >

        <TextView
            android:id="@+id/tv_leftpanel_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.31"
            android:padding="20dp"
            android:text="Menu"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffffff" />

        <ImageView
            android:id="@+id/img_leftmenu_close"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="10dp"
            android:src="@drawable/cancel" />
    </LinearLayout>

    <TextView
        android:id="@+id/tv_leftpanel_setting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Setting"
        android:textColor="#ffffff"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#ffffff" />

	<TextView
        android:id="@+id/tv_leftpanel_Rateus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Rate Us"
        android:textColor="#ffffff"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
            android:id="@+id/textView1adasd"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#ffffff" />
    
    <TextView
        android:id="@+id/tv_leftpanel_contact"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Contact"
        android:textColor="#ffffff"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
            android:id="@+id/textView1gdadasd"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#ffffff" />
    
</LinearLayout>
